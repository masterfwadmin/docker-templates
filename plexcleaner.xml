<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>plexcleaner</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>plexcleaner</Overview>
  <Description>
	  plexcleaner
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/masterfwadmin/plexcleaner</Registry>
  <Repository>masterfwadmin/plexcleaner</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
    </Publish>
