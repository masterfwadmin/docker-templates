<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>gmg</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>gmg</Overview>
  <Description>
	gmg
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/masterfwadmin/gmg/</Registry>
  <Repository>masterfwadmin/gmg</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>58888</HostPort> 
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
      <Variable>
        <Value></Value>
        <Name>GMG_GRILL_HOST</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value></Value>
        <Name>GMG_ALERTS_SLACK_WEBHOOKURL</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>99</Value>
        <Name>USER_ID</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>100</Value>
        <Name>GROUP_ID</Name>
        <Mode/>
      </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/gmg</HostDir>
      <ContainerDir>/app</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:58888]/</WebUI>
  <Banner></Banner>
  <Icon>https://raw.githubusercontent.com/masterfwadmin/gmg/master/assets/gmg.png</Icon>
</Container>
