<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>fluxvpn</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>fluxvpn</Overview>
  <Description>
	  fluxvpn
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/masterfwadmin/fluxvpn</Registry>
  <Repository>masterfwadmin/fluxvpn</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8688</HostPort> 
        <ContainerPort>8888</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8699</HostPort> 
        <ContainerPort>5900</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
      <Variable>
        <Value>192.168.1.0/24</Value>
        <Name>LAN_NETWORK</Name>
        <Mode/>
      </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/fluxvpn</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8688]/</WebUI>
  <Banner></Banner>
  <Icon>https://sites.duke.edu/dusontechtuesday/files/2020/04/vpn-150x150.png</Icon>
  <Config Name="User ID" Target="PUID" Default="1001" Mode="" Description="ID of the user the application runs as." Type="Variable" Display="advanced-hide" Required="true" Mask="false"/>
  <Config Name="Group ID" Target="PGID" Default="100" Mode="" Description="ID of the group the application runs as." Type="Variable" Display="advanced-hide" Required="true" Mask="false"/>
  <Config Name="Debug" Target="DEBUG" Default="false" Mode="" Description="Debugging" Type="Variable" Display="advanced-hide" Required="true" Mask="false"/>
  </Container>
