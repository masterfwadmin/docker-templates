<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>grafana</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>grafana</Overview>
  <Description>
	  grafana
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/masterfwadmin/grafana</Registry>
  <Repository>masterfwadmin/grafana</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3001</HostPort> 
        <ContainerPort>3001</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8086</HostPort> 
        <ContainerPort>8086</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
      <Variable>
        <Value>1001</Value>
        <Name>USER_ID</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>100</Value>
        <Name>GROUP_ID</Name>
        <Mode/>
      </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/grafana/influxdb</HostDir>
      <ContainerDir>/var/lib/influxdb</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/grafana/grafana</HostDir>
      <ContainerDir>/var/lib/grafana</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:3001]/</WebUI>
  <Banner></Banner>
  <Icon>https://cdn.icon-icons.com/icons2/2699/PNG/512/grafana_logo_icon_171048.png</Icon>
  </Container>
