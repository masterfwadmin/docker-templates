<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>elk-stack</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>iptvboss</Overview>
  <Description>
	  elk-stack
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/masterfwadmin/elk-stack</Registry>
  <Repository>masterfwadmin/elk-stack</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5601</HostPort> 
        <ContainerPort>5601</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9200</HostPort> 
        <ContainerPort>9200</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5044</HostPort> 
        <ContainerPort>5044</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
       <Port>
        <HostPort>5144</HostPort> 
        <ContainerPort>5144</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
       <Port>
        <HostPort>5141</HostPort> 
        <ContainerPort>5141</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5145</HostPort> 
        <ContainerPort>5145</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
      <Variable>
        <Value>1001</Value>
        <Name>USER_ID</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>100</Value>
        <Name>GROUP_ID</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>262144</Value>
        <Name>MAX_OPEN_FILES</Name>
        <Mode/>
      </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/elk-stack/conf.d</HostDir>
      <ContainerDir>/etc/logstash/conf.d</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5601]/</WebUI>
  <Banner></Banner>
  <Icon>https://cdn.iconscout.com/icon/free/png-256/elasticsearch-226094.png</Icon>
  </Container>
